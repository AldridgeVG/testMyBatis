<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="testMyBatis.mapper.SquareMapper">
    <select id="getSquare" parameterType="int" resultType="msquare">
        select length, height from dbsquare where length=#{id}
    </select>
</mapper>

